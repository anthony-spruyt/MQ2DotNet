{
	"Logging": {
		"LogLevel": {
			"Default": "Information"
		},
		"File": {
			"BasePath": "Logs\\MQFlux",
			"FileAccessMode": "KeepOpenAndAutoFlush",
			"FileEncodingName": "utf-8",
			"DateFormat": "yyyyMMdd",
			"CounterFormat": "000",
			"MaxFileSize": 10485760,
			"LogLevel": {
				"MQFlux": "Debug",
				"Default": "Information"
			},
			"IncludeScopes": true,
			"MaxQueueSize": 1,
			"Files": [
				{
					"Path": "MQFlux-<date:yyyy>-<date:MM>-<date:dd>-<counter>.log",
					"CounterFormat": "00"
				}
			]
		}
	}
}